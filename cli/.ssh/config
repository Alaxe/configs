Host *
  SetEnv TERM=xterm-256color

Host stg2
    HostName stg2.csail.mit.edu

Host stg2 *.mit.edu 128.52.* 128.30.*
    User alexalex
    GSSAPIAuthentication yes
    GSSAPIDelegateCredentials yes
    VerifyHostKeyDNS yes

Host jump.csail.mit.edu
    ControlMaster auto
    ControlPersist 604800
    ControlPath ${XDG_RUNTIME_DIR}/cm_socket-%C

Host *.csail.mit.edu stg2 !jump.csail.mit.edu 128.52.* 128.30.* 128.31.*
    # ProxyCommand ssh -W %h:%p mfa-jump.csail.mit.edu
    ProxyJump jump.csail.mit.edu
    GSSAPIDelegateCredentials yes
    ForwardAgent yes
